function [new scrambler] = untitled5( scrambler,size )
%Revalue scrambler using XOR operations
%   Detailed explanation goes here
LAST_VALUE = scrambler(size);
PRE_LAST_VALUE = scrambler(size - 1);

    LAST_ITEM = SCRAMBLER(SIZE);
    PRE_LAST_ITEM = SCRAMBLER(SIZE -1);
    for i=10:-1:2
        SCRAMBLER(i) = SCRAMBLER(i-1);
    end
    SCRAMBLER(1) = XOR(LAST_ITEM,PRE_LAST_ITEM);

SCR
end

